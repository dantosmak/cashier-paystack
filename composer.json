{
    "name": "wisdomanthoni/cashier-paystack",
    "description": "A Laravel Cashier Wrapper that provides an expressive, fluent interface to Paystack's subscription invoicing services.",
    "keywords": [
        "laravel",
        "paystack",
        "billing",
        "cashier"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Wisdom Anthony",
            "email": "wisdomanthoni@gmail.com"
        },
        {
            "name": "Oluwatosin Makinde",
            "email": "tmak247@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/database": "~5.8.0|^6.0|^7.0",
        "illuminate/support": "~5.8.0|^6.0|^7.0",
        "nesbot/carbon": "^2.0",
        "symfony/http-kernel": "~4.0",
        "unicodeveloper/laravel-paystack": "^1.0"
    },
    "require-dev": {
        "illuminate/http": "~5.8.0|^6.0|^7.0",
        "illuminate/routing": "~5.8.0|^6.0|^7.0",
        "illuminate/view": "~5.8.0|^6.0|^7.0",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Wisdomanthoni\\Cashier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Wisdomanthoni\\Cashier\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wisdomanthoni\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}